@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix rov: <http://www.w3.org/ns/reg-en-ovid#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Namespace voor de formele ontologie (de Klassen)
@prefix ont: <http://data.recht.nl/ont/> .

# Eigen namespaces voor het begrippenkader
@prefix vocab: <http://data.recht.nl/begrippenkader/> .

# Namespace voor de concepten (definities)
@prefix begrip: <http://data.recht.nl/begrip/> .

# Placeholder voor de Upper-Ontology
@prefix ob: <http://example.com/upper-ontology#> .

# === KLASSE DEFINITIES (Formele Ontologie) ===
ont:Mandaat
  a owl:Class ;
  rdfs:subClassOf ob:LegalRelator, ob:SocialRelator ;
  rdfs:isDefinedBy begrip:Mandaat .

ont:Bevoegdheid
  a owl:Class ;
  rdfs:subClassOf ob:LegalPosition, ob:Disposition ;
  rdfs:isDefinedBy begrip:Bevoegdheid .

ont:Bestuursorgaan
  a owl:Class ;
  rdfs:subClassOf ob:InstitutionalAgent, ob:GovernmentBody ;
  rdfs:isDefinedBy begrip:Bestuursorgaan .

ont:BesluitenNemen
  a owl:Class ;
  rdfs:subClassOf ob:InstitutionalAct, ob:CommunicativeAct ;
  rdfs:isDefinedBy begrip:BesluitenNemen .

# === SPECIALISEERDE RELATIES (Object Properties) ===
ont:heeftBevoegdheid
  a owl:ObjectProperty ;
  rdfs:subPropertyOf ob:inheresIn_2 ;
  rdfs:domain ont:Mandaat ;
  rdfs:range ont:Bevoegdheid ;
  rdfs:label "heeft bevoegdheid"@nl .

# === BEGRIP DEFINITIES (Gebruikersvriendelijk Begrippenkader) ===
begrip:Mandaat
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Mandaat"^^xsd:string ;
  skos:prefLabel "Mandaat"@nl ;
  skos:prefLabel "Mandate"@en ;
  skos:definition "De bevoegdheid om in naam van een bestuursorgaan besluiten te nemen."@nl ;
  skos:definition "The competence to make decisions on behalf of a governmental body."@en ;
  skos:example "Een ambtenaar neemt een besluit op grond van mandaat van het college van burgemeester en wethouders."@nl ;
  skos:counterExample "Een particulier kan geen besluiten nemen namens een bestuursorgaan."@nl ;
  skos:broader begrip:Bevoegdheid ;
  skos:related begrip:Bestuursorgaan, begrip:BesluitenNemen ;
dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:1&z=2025-09-01&g=2025-09-01> .

# === NIEUWE KLASSEN DEFINITIES - ART. 10:2 ANALYSE ===
ont:Mandaatgever
  a owl:Class ;
  rdfs:subClassOf ob:GovernmentBody ;
  rdfs:subClassOf ob:CollectiveSocialAgent ;
  rdfs:subClassOf ob:Offeror ;
  rdfs:subClassOf ob:Initiator ;
  rdfs:subClassOf ob:ValueBeholder ;
  rdfs:label "Mandaatgever"@nl ;
  rdfs:isDefinedBy begrip:Mandaatgever .

ont:Gemandateerde
  a owl:Class ;
  rdfs:subClassOf ob:LegalSubject ;
  rdfs:subClassOf ob:Claimant ;
  rdfs:subClassOf ob:PublicNaturalPerson ;
  rdfs:subClassOf ob:Executor ;
  rdfs:subClassOf ob:EligibleAgent ;
  rdfs:subClassOf ob:ValueBeneficiary ;
  rdfs:label "Gemandateerde"@nl ;
  rdfs:isDefinedBy begrip:Gemandateerde .

ont:Besluit
  a owl:Class ;
  rdfs:subClassOf ob:LegalRelator ;
  rdfs:subClassOf ob:ValueObject ;
  rdfs:subClassOf ob:ProductionFact ;
  rdfs:subClassOf ob:LegalCondition ;
  rdfs:label "Besluit"@nl ;
  rdfs:isDefinedBy begrip:Besluit .

ont:Artikel102_Structuur
  a owl:Class ;
  rdfs:subClassOf ob:Law ;
  rdfs:subClassOf ob:NormativeDescription ;
  rdfs:subClassOf ob:Jurisdiction ;
  rdfs:label "Artikel 10:2 Structuur"@nl ;
  rdfs:isDefinedBy begrip:Artikel102 .

# === EXTENDED KLASSEN - BASISONTOLOGY INTEGRATIES ===
ont:Mandaat
  rdfs:subClassOf ob:SocialRelator ;
  rdfs:subClassOf ob:Transaction ;
  rdfs:subClassOf ob:Offering ;
  rdfs:subClassOf ob:ValueAscription .

ont:Bevoegdheid
  rdfs:subClassOf ob:Power ;
  rdfs:subClassOf ob:SocialCommitment ;
  rdfs:subClassOf ob:ValueComponent .

ont:BesluitenNemen
  rdfs:subClassOf ob:TransactionEvent ;
  rdfs:subClassOf ob:Valuation ;
  rdfs:subClassOf ob:CClaim .

# === NIEUWE OBJECT PROPERTIES ===
ont:verleentMandaat
  a owl:ObjectProperty ;
  rdfs:subPropertyOf ob:inheresIn_2 ;
  rdfs:domain ont:Mandaatgever ;
  rdfs:range ont:Gemandateerde ;
  rdfs:label "verleent mandaat"@nl .

ont:leidtTotBesluit
  a owl:ObjectProperty ;
  rdfs:subPropertyOf ob:creates ;
  rdfs:domain ont:BesluitenNemen ;
  rdfs:range ont:Besluit ;
  rdfs:label "leidt tot besluit"@nl .

ont:heeftWaardeVoor
  a owl:ObjectProperty ;
  rdfs:subPropertyOf ob:inheresIn ;
  rdfs:domain ont:Besluit ;
  rdfs:range ob:ValueBeneficiary ;
  rdfs:label "heeft waarde voor"@nl .

ont:bevordertPubliekeWaarde
  a owl:ObjectProperty ;
  rdfs:subPropertyOf ob:creates ;
  rdfs:domain ont:Besluit ;
  rdfs:range ob:PublicValue ;
  rdfs:label "bevordert publieke waarde"@nl .

ont:valtBinnenHoofdstuk
  a owl:ObjectProperty ;
  rdfs:subPropertyOf ob:componentOf ;
  rdfs:domain ont:Artikel102_Structuur ;
  rdfs:range ob:Chapter ;
  rdfs:label "valt binnen hoofdstuk"@nl .

# === COMPLETE BEGRIP DEFINITIES ===
begrip:Mandaatgever
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Mandaatgever"^^xsd:string ;
  skos:prefLabel "Mandaatgever"@nl ;
  skos:prefLabel "Mandate Giver"@en ;
  skos:definition "Een bestuursorgaan dat mandaat verleent om vertegenwoordigers besluiten te laten nemen met volledige rechtskracht"@nl ;
  skos:definition "A governmental body that grants mandate allowing representatives to make decisions with full legal force"@en ;
  skos:scopeNote "De mandaatgever blijft verantwoordelijk voor besluiten genomen door gemandateerden"@nl ;
  skos:example "Een college van burgemeester en wethouders dat een ambtenaar mandaat verleent voor vergunningverlening"@nl ;
  skos:counterExample "Een particulier persoon kan geen mandaat verlenen voor overheidsbesluiten"@nl ;
  skos:broader begrip:GovernmentBody, begrip:Offeror ;
  skos:related begrip:Mandaat, begrip:Bevoegdheid, begrip:ValueBeholder ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:2&z=2025-09-01&g=2025-09-01> .

begrip:Gemandateerde
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Gemandateerde"^^xsd:string ;
  skos:prefLabel "Gemandateerde"@nl ;
  skos:prefLabel "Mandated Representative"@en ;
  skos:definition "Een vertegenwoordiger aan wie mandaat is verleend om besluiten te nemen namens het bestuursorgaan"@nl ;
  skos:definition "A representative who has been granted mandate to make decisions on behalf of the governmental body"@en ;
  skos:scopeNote "De gemandateerde oefent bevoegdheid uit binnen grenzen gesteld door de mandaatgever"@nl ;
  skos:example "Een ambtenaar die in naam van B&W vergunningen verleent"@nl ;
  skos:counterExample "Een adviseur zonder formeel mandaat kan geen besluiten nemen namens het orgaan"@nl ;
  skos:broader begrip:LegalSubject, begrip:PublicNaturalPerson, begrip:Claimant ;
  skos:related begrip:Mandaatgever, begrip:Bevoegdheid, begrip:ValueBeneficiary ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:2&z=2025-09-01&g=2025-09-01> .

begrip:Besluit
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Besluit"^^xsd:string ;
  skos:prefLabel "Besluit"@nl ;
  skos:prefLabel "Decision"@en ;
  skos:definition "Een juridisch bindende beslissing genomen in mandaatcontext die geldt als een besluit van het mandaatgever"@nl ;
  skos:definition "A legally binding decision made in mandate context that is deemed a decision of the mandate giver"@en ;
  skos:scopeNote "Besluiten genomen door vertegenwoordigers gelden als besluiten van het oorspronkelijke orgaan"@nl ;
  skos:example "Een vergunning verleend door een ambtenaar geldt als besluit van het college van B&W"@nl ;
  skos:counterExample "Een informele mening heeft geen status van een beslissing"@nl ;
  skos:broader begrip:LegalRelator ;
  skos:related begrip:BesluitenNemen, begrip:ProductionFact, begrip:LegalCondition ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:2&z=2025-09-01&g=2025-09-01> .

begrip:Artikel102
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Artikel102"^^xsd:string ;
  skos:prefLabel "Artikel 10:2 Awb"@nl ;
  skos:prefLabel "Article 10:2 Awb"@en ;
  skos:definition "Regelgeving die bepaalt dat besluiten genomen door vertegenwoordigers die mandaat hebben verkregen gelden als besluiten van de mandaatgever"@nl ;
  skos:definition "Regulation determining that decisions made by representatives who have been granted mandate are deemed decisions of the mandate giver"@en ;
  skos:scopeNote "Fundamentele regel voor vertegenwoordigend bestuur in de Nederlandse overheid"@nl ;
  skos:example "Artikel waarop mandaatverlening gebaseerd is"@nl ;
  skos:broader begrip:Law, begrip:NormativeDescription, begrip:Jurisdiction ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:2&z=2025-09-01&g=2025-09-01> .

begrip:Bevoegdheid
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Bevoegdheid"^^xsd:string ;
  skos:prefLabel "Bevoegdheid"@nl ;
  skos:prefLabel "Competence"@en ;
  skos:definition "De juridische positie die macht oplevert tot het nemen van institutionele acties."@nl ;
  skos:definition "A legal position enabling an agent to perform institutional acts."@en ;
  skos:example "Een burgemeester heeft bevoegdheid om bepaalde besluiten te nemen."@nl ;
  skos:counterExample "Een gewone burger heeft geen algemene bestuurlijke bevoegdheid."@nl ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:1&z=2025-09-01&g=2025-09-01> .

begrip:Bestuursorgaan
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-Bestuursorgaan"^^xsd:string ;
  skos:prefLabel "Bestuursorgaan"@nl ;
  skos:prefLabel "Governmental Body"@en ;
  skos:definition "Een instelling die bestuurlijke bevoegdheden uitoefent."@nl ;
  skos:definition "An institution that exercises governmental authority."@en ;
  skos:example "Het College van Burgemeester en Wethouders als bestuursorgaan."@nl ;
  skos:counterExample "De gemeenteraad oefent alleen bestuurlijke besluiten niet uit - dat is uitvoerend."@nl ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:1&z=2025-09-01&g=2025-09-01> .

begrip:BesluitenNemen
  a skos:Concept ;
  skos:inScheme vocab:JuridischBegrippenkader ;
  rov:status rov:stabiel ;
  dcterms:identifier "JBK-BesluitenNemen"^^xsd:string ;
  skos:prefLabel "Besluiten nemen"@nl ;
  skos:prefLabel "Making Decisions"@en ;
  skos:definition "Het nemen van bindende juridische beslissingen die gevolgen hebben."@nl ;
  skos:definition "Making binding legal decisions that have consequences."@en ;
  skos:example "Een ambtenaar neemt een vergunningbesluit namens het orgaan."@nl ;
  dcterms:source <https://wetten.overheid.nl/jci1.3:c:BWBR0005537&hoofdstuk=10&titeldeel=10.1&afdeling=10.1.1&artikel=10:1&z=2025-09-01&g=2025-09-01> .
