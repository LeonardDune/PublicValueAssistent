# === ONTOLOGYBASIS-BEGRIPPENKADER ===
# Geconsolideerd begrippenkader voor alle OntologyBasis concepten
# Volgend patroon zoals beschreven in de Tonto bestanden

voc:OntologyBasis
  a skos:ConceptScheme ;
  dcterms:title "OntologyBasis"@en ;
  dcterms:description "Begrippenkader voor de OntologyBasis ontologie"@nl ;
  dcterms:creator <https://pubvalue-definitions.nl/organization> ;
  owl:versionInfo "1.0.0" ;
  skos:hasTopConcept begrip:Individual, begrip:Agent, begrip:ValueBearer .

# === INDIVIDUAL ONTOLOGY ===
# All concepts are already documented and remain unchanged
begrip:Individual
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-Individual"^^xsd:string ;
  skos:prefLabel "Individual"@en ;
  skos:definition "A basic entity that can possess qualities and participate in relations."@en .

# Quality concept remains unchanged but documented here
begrip:Quality
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-Quality"^^xsd:string ;
  skos:prefLabel "Quality"@en ;
  skos:prefLabel "Kenmerk"@nl ;
  skos:definition "A Quality is an intrinsic property or characteristic that inheres in an Individual, enabling it to possess certain attributes or features."@en ;
  skos:definition "Een Kwaliteit is een intrinsieke eigenschap of kenmerk dat inherent is aan een Individu, waardoor het bepaalde attributen of kenmerken kan bezitten."@nl ;
  skos:scopeNote "A quality is a fundamental aspect of an individual that defines its characteristics and properties. Qualities are dependent on the individuals they inhere in and cannot exist independently. They can be perceivable (like color, shape, size) or non-perceivable (like mass, charge). Qualities are essential for distinguishing between different individuals and for understanding their behavior and interactions within a system."@en ;
  skos:example "color, shape, size, mass, charge, temperature, texture"@en .

# RELATION CONCEPTS FOR INDIVIDUAL ONTOLOGY
# Already documented above: inheresIn
# All IndividualOntology concepts are pre-existing.

# === AGENT ONTOLOGY ===

# CATEGORIES & KINDS
begrip:Agent
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-Agent"^^xsd:string ;
  skos:prefLabel "Agent"@en ;
  skos:prefLabel "Agent"@nl ;
  skos:definition "An individual capable of intentional actions who can be held accountable for their behavior."@en ;
  skos:definition "Een individu dat intentionele acties kan uitvoeren en verantwoordelijk kan worden gehouden voor zijn gedrag."@nl ;
  skos:broader begrip:Individual ;
  skos:topConceptOf voc:OntologyBasis .

# SOCIAL AGENTS
begrip:SocialAgent
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-SocialAgent"^^xsd:string ;
  skos:prefLabel "Social Agent"@en ;
  skos:prefLabel "Sociale Agent"@nl ;
  skos:definition "An agent that participates in social structures and collective intentional contexts."@en ;
  skos:definition "Een agent die deelneemt aan sociale structuren en collectieve intentionele contexten."@nl ;
  skos:broader begrip:Agent .

begrip:InstitutionalAgent
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-InstitutionalAgent"^^xsd:string ;
  skos:prefLabel "Institutional Agent"@en ;
  skos:prefLabel "Institutionele Agent"@nl ;
  skos:definition "A formal organization or institution that acts as a single agent with its own identity and agency."@en ;
  skos:definition "Een formele organisatie of instelling die als een enkele agent handelt met zijn eigen identiteit en agency."@nl ;
  skos:broader begrip:SocialAgent .

begrip:CollectiveSocialAgent
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-CollectiveSocialAgent"^^xsd:string ;
  skos:prefLabel "Collective Social Agent"@en ;
  skos:prefLabel "Collectieve Sociale Agent"@nl ;
  skos:definition "A collective of social agents that acts as a single agent with emergent group agency."@en ;
  skos:definition "Een collectief van sociale agenten die als een enkele agent handelt met emergente groepsagency."@nl ;
  skos:broader begrip:SocialAgent .

begrip:Society
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-Society"^^xsd:string ;
  skos:prefLabel "Society"@en ;
  skos:prefLabel "Samenleving"@nl ;
  skos:definition "A human society as a collective social agent with shared norms and institutions."@en ;
  skos:definition "Een menselijke samenleving als collectieve sociale agent met gedeelde normen en instituties."@nl ;
  skos:broader begrip:CollectiveSocialAgent .

begrip:Ecosystem
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-Ecosystem"^^xsd:string ;
  skos:prefLabel "Ecosystem"@en ;
  skos:prefLabel "Ecosysteem"@nl ;
  skos:definition "A social ecosystem as a collective agent with interdependent networks and systemic behavior."@en ;
  skos:definition "Een sociaal ecosysteem als collectieve agent met interdependent netwerken en systemisch gedrag."@nl ;
  skos:broader begrip:CollectiveSocialAgent .

# PHYSICAL AGENTS
begrip:PhysicalAgent
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-PhysicalAgent"^^xsd:string ;
  skos:prefLabel "Physical Agent"@en ;
  skos:prefLabel "Fysische Agent"@nl ;
  skos:definition "An individual physical entity capable of intentional action as an autonomous agent."@en ;
  skos:definition "Een individueel fysiek individu dat intentioneel kan handelen als een autonome agent."@nl ;
  skos:broader begrip:Agent .

begrip:NaturalPerson
  a skos:Concept ;
  skos:inScheme voc:OntologyBasis ;
  rov:status rov:stabiel ;
  dcterms:identifier "Basis-NaturalPerson"^^xsd:string ;
  skos:prefLabel "Natural Person"@en ;
  skos:prefLabel "Natuurlijk Persoon"@nl ;
  skos:definition "A physical agent with full legal capacity as a natural person, entitled to rights and subject to duties"@en ;
  skos:definition "Een fysieke agent met volledige juridische capaciteit als natuurlijk persoon, gerechtigd tot rechten en onderworpen aan plichten"@nl ;
